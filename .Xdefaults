URxvt*loginShell:true
URxvt*transparent: false
URxvt*shading:5
URxvt*scrollBar:true
URxvt*scrollBar_right:true
URxvt*scrollBar_floating:false
URxvt*scrollstyle:plain
URxvt*secondaryScroll:false
URxvt.cursorBlink: true
#URxvt*meta8:true
URxvt.modifier: meta

URxvt*saveLines:32767
URxvt*throughColor:#000000
URxvt*scrollColor:#B0B0B0
URxvt.depth: 32
URxvt*urgentOnBell:true
URxvt*perl-ext-common: default,matcher,-option-popup,-selection-popup,-readline
URxvt*matcher.button:1
URxvt*urlLauncher: google-chrome-stable
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  google-chrome
URxvt.url-select.underline: true
URxvt.keysym.C-Escape:      perl:keyboard-select:activate
URxvt.keysym.C-s:           perl:keyboard-select:search
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
Xft*hinting:true
Xft*hintstyle:hintslight
Xft*antialias: true
Xft*rgba: rgb
URxvt*buffered: false
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True

! Fonts
URxvt.allow_bold:true
URxvt*font: xft:Monospace:pixelsize=14
URxvt*boldFont: xft:Monospace:pixelsize=14
 
! Fix font space
URxvt*letterSpace: 0


!Colors
!Black
URxvt.color0: #1B1B1B
URxvt.color8: #5D5D5D

!Red
URxvt.color1: #EA6868
URxvt.color9: #E09690

!Green
URxvt.color2: #8EB338
URxvt.color10: #CDEE69

!Yellow
URxvt.color3: #D0B03C
URxvt.color11: #FFE999

!Blue
URxvt.color4:  #8AD1F9
URxvt.color12: #B0E2FF

!Magenta
URxvt.color5:  #C9A0D1
URxvt.color13: #FBB1F9

!Cyan
URxvt.color6:  #218693
URxvt.color14: #77DFD8

!White
URxvt.color7: #B0B0B0
URxvt.color15: #F7F7F7

URxvt.background: #262626
URxvt.colorBD: #FFFFFF
URxvt.cursorColor: #A0A0A0
URxvt.cursorColor2: #FFFFFF
URxvt.foreground: #F7F7F7
URxvt*throughColor:#000000
URxvt*scrollColor:#B0B0B0
URxvt.jumpScroll:true
