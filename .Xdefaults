URxvt*loginShell:true
URxvt*transparent: false
URxvt*inheritPixmap: true
URxvt*shading:5
! URxvt*background:Black
! URxvt*foreground:White
URxvt*scrollBar:true
URxvt*scrollBar_right:true
URxvt*scrollBar_floating:false
URxvt*scrollstyle:plain
URxvt*secondaryScroll:false
URxvt.cursorBlink: true
#URxvt*meta8:true
URxvt.modifier: meta

URxvt*saveLines:32767
URxvt*throughColor:#000000
URxvt*scrollColor:#222222
URxvt.depth: 32
URxvt*font: xft:bitstream vera sans mono:bold:pixelsize=15:antialias=true
URxvt*urgentOnBell:true
URxvt*perl-ext-common: default,matcher,-option-popup,-selection-popup,-readline
URxvt*matcher.button:1
URxvt*urlLauncher: google-chrome-stable
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  google-chrome
URxvt.url-select.underline: true
URxvt.keysym.C-Escape:      perl:keyboard-select:activate
URxvt.keysym.C-s:           perl:keyboard-select:search
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
Xft*hinting:true
Xft*hintstyle:hintslight
Xft*antialias: true
Xft*rgba: rgb
URxvt*buffered: false
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True

